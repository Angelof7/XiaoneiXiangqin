<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<!-- name类名，table表名 -->
	<class name="com.xiangqin.ORM.UserInfo" table="userinfo">
		<!-- id标签表示主键 -->
		<id name="studentcard" column="studentcard" type="string">
			<!--  -->
			<generator class="assigned"></generator>
		</id>
		<!-- 下面是字段映射 name属性,column是字段 type数据库类型 -->
		<property name="studentname" column="studentname" type="string"></property>
		<property name="sex" column="sex" type="string"></property>
		<property name="address" column="address" type="string"></property>
		<property name="signuature" column="signuature" type="string"></property>
	</class>
</hibernate-mapping>