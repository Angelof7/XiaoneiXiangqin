		<property name="marriageStatus" column="MarriageStatus" type="int" />
		<property name="objectType" column="ObjectType" type="int" />
		<property name="gender" column="Gender" type="int" />
		<property name="age" column="Age" type="int" />
		<property name="height" column="Height" type="int" />
		<property name="weight" column="Weight" type="int" />
		<property name="liveLocation" column="LiveLocation" type="string" />
		<property name="education" column="Education" type="int" />
		<property name="graduateFrom" column="GraduateFrom" type="string" />
		<property name="industry" column="Industry" type="int" />
		<property name="company" column="Company" type="string" />
		<property name="currentJob" column="CurrentJob" type="int" />
		<property name="monthlyIncome" column="MonthlyIncome" type="int" />
		<property name="account" column="Account" type="string" />
		<property name="housingCondition" column="HousingCondition" type="int" />
		<property name="carCondition" column="CarCondition" type="int" />
		<property name="ethnic" column="Ethnic" type="string" />
		<property name="birthLocation" column="BirthLocation" type="string" />
		<property name="homeRanking" column="HomeRanking" type="int" />
		<property name="haveChildren" column="HaveChildren" type="int" />
		<property name="constellation" column="Constellation" type="int" />
		<property name="bloodType" column="BloodType" type="int" />
		<property name="zodiac" column="Zodiac" type="int" />
		<property name="religion" column="Religion" type="int" />
		<property name="weiboURL" column="WeiBoURL" type="string" />
		<property name="doubanURL" column="DouBanURL" type="string" />
	</class>
</hibernate-mapping>